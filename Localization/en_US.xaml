<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <sys:String x:Key="OverlaySettings_Title">Overlay Settings</sys:String>
    <sys:String x:Key="OverlaySettings_ControllerShortcut">Controller Shortcut</sys:String>
    <sys:String x:Key="OverlaySettings_GuideControllerShortcut">Note for using Guide Button for controller activation: Ensure you disable Guide Button detection in all other programs. Check Steam and Xbox GameBar settings and inside playnite, disable 'Show Playnite When Guide Button is Pressed'</sys:String>
    <sys:String x:Key="OverlaySettings_GuideControllerShortcut_kbnote">Note: The keyboard shortcut is Alt + Backtick</sys:String>
    <sys:String x:Key="OverlaySettings_CoverShape">Cover Shape</sys:String>
    <sys:String x:Key="OverlaySettings_EnableDebugMode">Enable Debug Mode</sys:String>
    <sys:String x:Key="OverlaySettings_DebugMode_Note">Shows additional debugging information in the overlay. Useful if a game is not able to be exited. Also causes the overlay to output a LOAD of logs to the debugger console.</sys:String>
    <sys:String x:Key="OverlaySettings_Buttons">Buttons</sys:String>
    <sys:String x:Key="OverlaySettings_GuideButtons">Keyboard shortcuts are written in SendKeys format. Ctrl is written as '^', Shift as '+', and Alt as '%'. See more info</sys:String>
    <sys:String x:Key="OverlaySettings_DiscordButton">Show Discord Button</sys:String>
    <sys:String x:Key="OverlaySettings_DiscordButtonDesc">Note: Discord launches really weirdly so path is not customizable</sys:String>
    <sys:String x:Key="OverlaySettings_RecordButton">Show Record Gameplay Button</sys:String>
    <sys:String x:Key="OverlaySettings_RecentRecordButton">Show Recent Record Gameplay Button</sys:String>
    <sys:String x:Key="OverlaySettings_StreamingButton">Show Streaming Button</sys:String>
    <sys:String x:Key="OverlaySettings_PerfomanceButton">Show Perfomance Overlay Button</sys:String>
    <sys:String x:Key="OverlaySettings_ScreenshotButton">Show Screenshot Gallery Button</sys:String>
    <sys:String x:Key="OverlaySettings_BrowserButton">Show Web Browser Button</sys:String>
    <sys:String x:Key="OverlaySettings_btn_Shortcut">Shortcut:</sys:String>
    <sys:String x:Key="OverlaySettings_btn_Directory">Directory:</sys:String>
    <sys:String x:Key="OverlaySettings_btn_BrowserPath">Browser Path:</sys:String>
    <sys:String x:Key="Overlay_ReturnToGame">Return to Game</sys:String>
    <sys:String x:Key="Overlay_ShowPlaynite">Show Playnite</sys:String>
    <sys:String x:Key="Overlay_ExitGame">Exit Game</sys:String>
</ResourceDictionary>